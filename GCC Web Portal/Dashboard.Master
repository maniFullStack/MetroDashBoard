								<li><a href="/Reports/Monthly" title="Monthly Report"><i class="fa fa-calendar"></i> Monthly Report</a></li>
								<li><a href="/Reports/MonthlyHotel" title="Monthly Hotel Report"><i class="fa fa-calendar"></i> Monthly Hotel Report</a></li>
								<li><a href="/Reports/Quarterly" title="Quarterly Report"><i class="fa fa-pie-chart"></i> Quarterly Report</a></li>
								<li><a href="/RespondentProfile" title="Respondent Profile"><i class="fa fa-users"></i> Respondent Profile</a></li>
								<li><a href="/GuestDetails" title="Respondent Search"><i class="fa fa-user"></i> Respondent Search</a></li>
								<% } %>
								<% if ( User.Group == UserGroups.PropertyStaff ) { %>
								<li><a href="/RespondentProfile" title="Respondent Profile"><i class="fa fa-users"></i> Respondent Profile</a></li>
								<li><a href="/GuestDetails" title="Respondent Search"><i class="fa fa-user"></i> Respondent Search</a></li>
								<% } %>
