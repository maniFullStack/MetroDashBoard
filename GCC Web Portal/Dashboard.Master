								<li><a href="/SnapshotExport" title="Snapshot Report"><i class="fa fa-camera"></i> Snapshot Report</a></li>
								<% } %>
								<% if ( new UserGroups[] { UserGroups.ForumAdmin, UserGroups.CorporateManagement, UserGroups.CorporateMarketing, UserGroups.PropertyManagers }.Contains( User.Group ) ) { %>
								<li><a href="/QuestionTopBottom/" title="Top / Bottom Questions"><i class="fa fa-long-arrow-up"></i>Top / <i class="fa fa-long-arrow-down"></i>Bottom Questions</a></li>
