						<li class="header">FILTERS</li>
						<li><a href="#" id="btnAddFilter" data-toggle="modal" data-target="#filter-modal"><i class='fa fa-plus-circle'></i><span><%= ( ActiveFilters == 0 ? "Add" : "Edit" ) %> Filter(s)</span></a></li>
						<% if (ActiveFilters > 0) { %>
						<li class="treeview active">
							<a href="#"><i class='fa fa-filter'></i><span>Active Filters</span> <span class="label label-primary pull-right"><%= ActiveFilters %></span></a>
							<ul class="treeview-menu filter-list">
								<%
								foreach (IReportFilter flt in Filters) {
									if ( flt.IsActive && !IsFilterHidden( flt ) ) { %>
									<li>
										<span><%= flt.Label %>: <%= ReportingTools.CleanData( flt.GetSelectedFilterText() ) %></span>
										<% if ( !flt.ID.Equals( "fltDateRange" )
											   && !( IsPropertyUser && flt.ID.Equals( "fltProperty" ) ) ) { %>
										<a href="?rm=<%= flt.ID %>" class="btn btn-box-tool pull-right" style="position:absolute;top:0;right:5px;"><i class="fa fa-times"></i></a>
										<% } %>
									</li>
								<%  }
								} %>
							</ul>
						</li>
						<% } %>
					</ul>
					<% } %>
					<% if ( !String.IsNullOrWhiteSpace( lblRecordCount.Text ) ) { %>
					<ul class="sidebar-menu">
						<li class="header">DETAILS</li>
					</ul>
					<div class="detail-list">
						Records: <asp:Label runat="server" ID="lblRecordCount"></asp:Label>
					</div>
					<% } %>
					<!-- /.sidebar-menu -->
				</section>
				<!-- /.sidebar -->
			</aside>

			<!-- Content Wrapper. Contains page content -->
			<div class="content-wrapper">
